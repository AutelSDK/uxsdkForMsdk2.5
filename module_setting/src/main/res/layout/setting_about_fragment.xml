<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="@dimen/common_450dp"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <com.autel.common.widget.CommonItemText
            android:id="@+id/about_app_version"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:bottom_line_visibility="visible"
            android:background="@drawable/common_item_bg_only_top"
            app:end_view_arrow_is_show="invisible"
            app:end_view_text="@string/common_text_no_value"
            app:title_name="@string/common_text_app_version" />


        <com.autel.common.widget.CommonItemText
            android:id="@+id/cit_aircraft_version"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/common_item_bg_none"
            app:bottom_line_visibility="visible"
            app:drawableEnd="@drawable/common_ic_arrow_right_white"
            app:end_view_arrow_is_show="visible"
            app:end_view_text="@string/common_text_no_value"
            app:title_name="@string/common_text_aircraft_version_title" />

        <com.autel.common.widget.CommonItemText
            android:id="@+id/cit_remote_version"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/common_item_bg_none"
            app:bottom_line_visibility="visible"
            app:drawableEnd="@drawable/common_ic_arrow_right_white"
            app:end_view_arrow_is_show="visible"
            app:end_view_text="@string/common_text_no_value"
            app:title_name="@string/common_text_remote_version__title" />

        <com.autel.common.widget.CommonItemText
            android:id="@+id/cit_gimbal_version"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/common_item_bg_none"
            app:bottom_line_visibility="visible"
            app:drawableEnd="@drawable/common_ic_arrow_right_white"
            app:end_view_arrow_is_show="invisible"
            app:end_view_text="@string/common_text_no_value"
            app:title_name="@string/common_text_gimbal_version_title" />

        <com.autel.common.widget.CommonItemText
            android:id="@+id/cit_battery_version"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/common_item_bg_none"
            app:bottom_line_visibility="visible"
            app:drawableEnd="@drawable/common_ic_arrow_right_white"
            app:end_view_arrow_is_show="invisible"
            app:end_view_text="@string/common_text_no_value"
            app:title_name="@string/common_text_battery_version_title" />

        <com.autel.common.widget.CommonItemText
            android:id="@+id/cit_loud_speaker"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/common_item_bg_none"
            android:visibility="gone"
            app:bottom_line_visibility="visible"
            app:drawableEnd="@drawable/common_ic_arrow_right_white"
            app:end_view_arrow_is_show="invisible"
            app:end_view_text="@string/common_text_no_value"
            app:title_name="@string/common_text_loud_speaker" />

        <com.autel.common.widget.CommonItemText
            android:id="@+id/update_app_version"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:bottom_line_visibility="visible"
            android:background="@drawable/common_item_bg_none"
            app:drawableEnd="@drawable/common_ic_arrow_right_white"
            app:end_view_arrow_is_show="visible"
            app:title_name="@string/common_app_upgrade" />
        <com.autel.common.widget.CommonItemText
            android:id="@+id/update_firmware_version"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:bottom_line_visibility="invisible"
            android:background="@drawable/common_item_bg_only_bottom"
            app:drawableEnd="@drawable/common_ic_arrow_right_white"
            app:end_view_arrow_is_show="visible"
            app:title_name="@string/common_text_upgrade" />

        <com.autel.common.widget.CommonItemText
            android:id="@+id/about_fly_serial_number"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:bottom_line_visibility="visible"
            app:end_view_arrow_is_show="invisible"
            android:background="@drawable/common_item_bg_only_top"
            android:layout_marginTop="@dimen/common_10dp"
            app:end_view_text="@string/common_text_no_value"
            app:title_name="@string/common_text_fly_serial_number" />

        <com.autel.common.widget.CommonItemText
            android:id="@+id/about_controller_serial_number"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:bottom_line_visibility="visible"
            app:end_view_arrow_is_show="invisible"
            app:end_view_text="@string/common_text_no_value"
            android:background="@drawable/common_item_bg_none"
            app:title_name="@string/common_text_controller_serial_number" />

        <com.autel.common.widget.CommonItemText
            android:id="@+id/about_gimbal_serial_number"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:bottom_line_visibility="visible"
            app:end_view_arrow_is_show="invisible"
            android:background="@drawable/common_item_bg_none"
            app:end_view_text="@string/common_text_no_value"
            app:title_name="@string/common_text_gimbal_serial_number" />

        <com.autel.common.widget.CommonItemText
            android:id="@+id/about_battery_version"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:bottom_line_visibility="invisible"
            app:end_view_arrow_is_show="invisible"
            android:background="@drawable/common_item_bg_only_bottom"
            app:end_view_text="@string/common_text_no_value"
            app:title_name="@string/common_text_batterY_1_version" />




    </LinearLayout>
</androidx.core.widget.NestedScrollView>